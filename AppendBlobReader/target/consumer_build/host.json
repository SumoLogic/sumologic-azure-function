{
    "serviceBus": {
      "maxConcurrentCalls": 20,
      "prefetchCount": 80,
      "autoRenewTimeout": "00:10:00",
      "autoComplete": true
    },
    "functionTimeout": "00:10:00",
    "functions": ["BlobTaskConsumer"],
    "applicationInsights": {
        "sampling": {
          "isEnabled": false,
          "maxTelemetryItemsPerSecond" : 20000
        }
    },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": false
        }
      },
      "logLevel": {
        "default": "Warning",
        "Host.Aggregator": "Trace",
        "Host.Results": "Information",
        "Function": "Information"
      }
    }
}
