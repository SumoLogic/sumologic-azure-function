{
    "version": "2.0",
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.0.0, 5.0.0)"
    },
    "serviceBus": {
      "maxConcurrentCalls": 40,
      "prefetchCount": 80,
      "autoRenewTimeout": "00:10:00",
      "autoComplete": true
    },
    "functionTimeout": "00:10:00",
    "functions": ["AppendBlobTaskConsumer"],
    "applicationInsights": {
        "sampling": {
          "isEnabled": false,
          "maxTelemetryItemsPerSecond" : 20000
        }
    },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": false
        }
      },
      "logLevel": {
        "default": "Warning",
        "Host.Aggregator": "Trace",
        "Host.Results": "Information",
        "Function": "Trace"
      }
    }
}
