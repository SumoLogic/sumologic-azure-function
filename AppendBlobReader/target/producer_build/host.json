{
  "eventHub": {
    "maxBatchSize": 400,
    "prefetchCount": 800,
    "batchCheckpointFrequency": 400
  },
  "functionTimeout": "00:30:00",
  "functions": [
    "BlobTaskProducer"
  ],
  "applicationInsights": {
    "sampling": {
      "isEnabled": true,
      "maxTelemetryItemsPerSecond": 20000
    }
  },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": false
        }
      },
      "logLevel": {
        "default": "Warning",
        "Host.Aggregator": "Trace",
        "Host.Results": "Information",
        "Function": "Information"
      }
    }
}
