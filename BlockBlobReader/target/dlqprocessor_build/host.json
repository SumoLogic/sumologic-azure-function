{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "functionTimeout": "00:10:00",
  "functions": ["DLQTaskConsumer"],
  "applicationInsights": {
    "sampling": {
      "isEnabled": false,
      "maxTelemetryItemsPerSecond" : 20000
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    },
    "logLevel": {
      "default": "Warning",
      "Host.Aggregator": "Trace",
      "Host.Results": "Information",
      "Function": "Trace"
    }
  }
}
